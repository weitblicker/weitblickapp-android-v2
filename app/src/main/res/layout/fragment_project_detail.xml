            <ListView
                android:id="@+id/events"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/orange7"
                app:layout_constraintTop_toBottomOf="@+id/orange7">

            </ListView>

            <ListView
                android:id="@+id/news"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/orange7"
