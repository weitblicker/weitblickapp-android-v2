<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ic_green">

        <TextView
            android:id="@+id/faqtext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="93dp"
            android:fontFamily="@font/opensansbold"
            android:text="FAQ"
            android:textColor="@color/black"
            android:textSize="20dp"
            app:layout_constraintStart_toEndOf="@+id/faq"
            app:layout_constraintTop_toBottomOf="@+id/imageView19" />

        <ImageView
            android:id="@+id/faq"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="46dp"
            android:layout_marginTop="30dp"
            android:alpha="0.8"
            android:tint="#333333"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/news"
            app:srcCompat="@drawable/ic_menu_manage" />

        <ImageView
            android:id="@+id/contact"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="46dp"
            android:layout_marginTop="30dp"
            android:alpha="0.8"
            android:tint="#333333"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/faq"
            app:srcCompat="@drawable/ic_menu_manage" />

        <TextView
            android:id="@+id/contactext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="153dp"
            android:fontFamily="@font/opensansbold"
            android:text="Kontakt"
            android:textColor="@color/black"
            android:textSize="20dp"
            app:layout_constraintStart_toEndOf="@+id/contact"
            app:layout_constraintTop_toBottomOf="@+id/imageView19" />

        <TextView
            android:id="@+id/agbtext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="11dp"
            android:layout_marginTop="33dp"
            android:fontFamily="@font/opensansbold"
            android:text="AGB"
            android:textColor="@color/black"
            android:textSize="20dp"
            app:layout_constraintStart_toEndOf="@+id/news"
            app:layout_constraintTop_toBottomOf="@+id/imageView19" />

        <ImageView
            android:id="@+id/news"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="46dp"
            android:layout_marginTop="30dp"
            android:alpha="0.8"
            android:tint="#333333"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView19"
            app:srcCompat="@drawable/ic_menu_manage" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="match_parent"
            android:layout_height="230dp"
            android:layout_alignParentTop="true"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@drawable/ic_wbcd_logo_standard_black_font" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>